{"fieldId": "pid"} -> @options NodeDataSet
{"fieldId": "pid"} -> @options EdgeDataSet
Actor addNode -> in NodeModel out -> add NodeDataSet
Actor addLink -> in EdgeModel out -> add EdgeDataSet

