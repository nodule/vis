EdgeModel(object/copy)
NodeModel(object/copy)
["source.pid","target.pid"] -> @from EdgeModel
["from","to"]               ->   @to EdgeModel

["pid","title"] -> @from NodeModel
["id","label"]  ->   @to NodeModel

